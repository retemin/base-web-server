.nav-tabs{
	background-color: white;
	padding-top: 4px;
}
.nav-tabs>li:first-child{
	margin-left: 30px;
}
.nav-tabs>li>a{
	height: 38px;
	border-radius:0 0 0 0;
}
.nav-tabs>li.active>a{
	border-top:3px solid #0f83e0;
	color: #0f83e0;
	border-radius ： none;
}

.nav-tabs>li.active>a:hover{
	border-top:3px solid #31cb59;
	color: #31cb59;
}

