.btn-info,.btn-success {
    background-color: #fda720;
    border-color: #fda720;
    color: #FFFFFF;
}
.btn-primary{
    background-color: #31cb59;
    border-color: #31cb59;
    color: #FFFFFF;
}
.btn-info:hover,.btn-success:hover,.btn-primary:hover {
    background-color: #0f83e0;
    border-color: #0f83e0;
    color: #FFFFFF;
}

